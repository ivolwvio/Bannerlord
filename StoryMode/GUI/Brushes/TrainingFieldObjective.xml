<Brushes>
  <Brush Name="Objective.Completion" >
    <Layers>
      <BrushLayer Name="Default" Sprite="BlankWhiteCircle" AlphaFactor="0.7" />
    </Layers>
    <Styles>
      <Style Name="Default" >
        <BrushLayer Name="Default" Color="#F4E1C4FF"/>
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" Color="#F39C12FF" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" Color="#2ECC71FF" />
      </Style>
    </Styles>
  </Brush>

  <Brush Name="Objective.Text" Font="FiraSansExtraCondensed-Light" TextHorizontalAlignment="Left" TextVerticalAlignment="Center">
    <Styles>
      <Style Name="Default" FontSize="24" FontColor="#F4E1C4FF" TextOutlineColor="#00000000" TextBlur="0.1" TextOutlineAmount="0" TextGlowRadius="0.1"/>
      <Style Name="Selected" FontSize="24" FontColor="#F39C12FF" TextOutlineColor="#00000000" TextBlur="0.1" TextOutlineAmount="0" TextGlowRadius="0.1"/>
      <Style Name="Disabled" FontSize="24" FontColor="#2ECC71FF" TextOutlineColor="#00000000" TextBlur="0.1" TextOutlineAmount="0" TextGlowRadius="0.1"/>
    </Styles>
  </Brush>

  <Brush Name="Objective.Text.WithOutline" Font="FiraSansExtraCondensed-Light" TextHorizontalAlignment="Left" TextVerticalAlignment="Center">
    <Styles>
      <Style Name="Default" FontSize="24" FontColor="#F4E1C4FF" TextOutlineColor="#000000FF" TextBlur="0.5" TextOutlineAmount="0.01" TextGlowRadius="0.1"/>
    </Styles>
  </Brush>


  <Brush Name="Training.Main.Text" Font="Galahad" TextHorizontalAlignment="Center" TextVerticalAlignment="Center">
    <Styles>
      <Style Name="Default" FontSize="42" FontColor="#F4E1C4FF" TextOutlineColor="#00000000" TextBlur="0.7" TextOutlineAmount="0.1" TextGlowRadius="0.1"/>
    </Styles>
  </Brush>

</Brushes>