      <Style Name="Default" FontColor="#FAF4DEFF" TextGlowColor="#000000FF" FontSize="35" />
    </Styles>
  </Brush>
  <Brush Name="BoardGame.InstructionsTitle.Text">
    <Layers>
    </Layers>
    <Styles>
      <Style Name="Default" FontColor="#D8BA86FF" TextGlowColor="#000000FF" FontSize="40" />
