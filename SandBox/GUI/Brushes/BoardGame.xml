<Brushes>
  <Brush Name="BoardGame.Dice">
    <Layers>
      <BrushLayer Name="Default" Sprite="Disabled" Color="#33FF22FF" AlphaFactor="0.3" />
    </Layers>
    <Styles>
      <Style Name="Default">
      </Style>
    </Styles>
  </Brush>
  <Brush Name="BoardGame.Button" FontColor="#FAF4DEFF">
    <Layers>
      <BrushLayer Name="Default" Sprite="SPScoreboard\bottom_button" Color="#FFFFFFFF" AlphaFactor="0.8" />
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" ColorFactor="1.0" Sprite="SPScoreboard\bottom_button" />
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" ColorFactor="1.2" Sprite="SPScoreboard\bottom_button" />
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" ColorFactor="0.85" Sprite="SPScoreboard\bottom_button" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" ColorFactor="0.4" Sprite="SPScoreboard\bottom_button" />
      </Style>
      <Style Name="Selected">
        <BrushLayer Name="Default" ColorFactor="1.8" Sprite="SPScoreboard\bottom_button" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="BoardGame.Button.Text">
    <Layers>
    </Layers>
    <Styles>
      <Style Name="Default" FontColor="#FAF4DEFF" TextGlowColor="#000000FF" FontSize="20" />
